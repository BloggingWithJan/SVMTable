<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<P13nDialog showReset="true" showResetEnabled="{path: '/showResetEnabled'}" ok="._onP13nOK" cancel="._onP13nCancel" reset="._onP13nReset">
		<panels>
			<P13nColumnsPanel changeColumnsItems="._onP13nChangeColumnItems" items="{path: '/items'}" columnsItems="{path: '/columnItems'}">
				<items>
					<P13nItem columnKey="{columnKey}" text="{text}"/>
				</items>
				<columnsItems>
					<P13nColumnsItem columnKey="{columnKey}" index="{index}" visible="{visible}"/>
				</columnsItems>
			</P13nColumnsPanel>
		</panels>
	</P13nDialog>
</core:FragmentDefinition>